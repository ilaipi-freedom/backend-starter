{
    // 默认格式化工具选择prettier
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    // 保存的时候自动格式化
    // "editor.formatOnSave": true,
    //开启自动修复
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.fixAll.eslint": "explicit"
    },
    // 配置stylelint检查的文件类型范围
    "stylelint.validate": ["ts"], // 与package.json的scripts对应
    "stylelint.enable": true,
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "[shellscript]": {
      "editor.defaultFormatter": "trae.ai-code-completion"
    },
    "[dotenv]": {
      "editor.defaultFormatter": "trae.ai-code-completion"
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.words": []
  }
  